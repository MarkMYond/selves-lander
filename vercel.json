{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "npm install --include=optional",
  "functions": {
    "**/*.func": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "https://payload-sigma-ten.vercel.app/admin/$1"
    }
  ],
  "env": {
    "NUXT_PUBLIC_PAYLOAD_API_URL": "https://cms.taash.ai"
  },
  "github": {
    "silent": true,
    "autoAlias": true,
    "autoJobCancelation": true
  }
}
